.icon-star:before {
  content: "\e98d";
}
.icon-search:before {
  content: "\e6c8";
}
.star-container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: -8rpx;
}
.star-container .star-container-item {
  height: 20rpx;
  line-height: 20rpx;
}
.star-container .star-container-item .star-active {
  color: #ffcb3c;
  font-size: 18rpx;
}
.star-container .star-container-item .star-unactive {
  font-size: 18rpx;
  color: #e4e4e4;
}
.movie-container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.movie-container .movie-container-item {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin: 20rpx;
  width: calc(33.33% - 40rpx);
}
.movie-container .movie-container-item .movie-container-item-image {
  width: 100%;
  height: 270rpx;
}
.movie-container .movie-container-item .movie-container-item-title {
  margin-top: 26rpx;
  color: #666;
  font-size: 24rpx;
}
.movie-container .movie-container-item .movie-container-item-footer {
  margin-top: 28rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.movie-container .movie-container-item .movie-container-item-footer .movie-container-item-footer-commentNum {
  margin-left: 20rpx;
  color: #666;
  font-size: 18rpx;
}
