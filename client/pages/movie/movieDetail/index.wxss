.icon-delete:before {
  content: "\e620";
}
.icon-star:before {
  content: "\e98d";
}
.icon-search:before {
  content: "\e6c8";
}
.icon-arrow:before {
  content: "\e6e9";
}
.star-container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: -8rpx;
}
.star-container .star-container-item {
  height: 20rpx;
  line-height: 20rpx;
}
.star-container .star-container-item .star-active {
  color: #ffcb3c;
  font-size: 18rpx;
}
.star-container .star-container-item .star-unactive {
  font-size: 18rpx;
  color: #e4e4e4;
}
.container {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}
.container .poster {
  width: 100%;
  position: relative;
}
.container .poster .poster-image {
  width: 100%;
  height: 400rpx;
  -webkit-filter: blur(20px);
}
.container .poster .poster-content {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 40rpx;
}
.container .poster .poster-content .poster-content-title {
  margin-top: 20rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #fff;
}
.container .poster .poster-content .poster-content-info {
  display: flex;
  flex-direction: row;
  margin-top: 40rpx;
  font-size: 32rpx;
  color: #fff;
}
.container .poster .poster-content .poster-content-info .poster-content-info-country {
  font-size: 28rpx;
  color: #fff;
}
.container .poster .poster-content .poster-content-info .poster-content-info-year {
  margin-left: 40rpx;
}
.container .poster .poster-content .poster-content-count {
  margin-top: 70rpx;
  display: flex;
  flex-direction: row;
}
.container .poster .poster-content .poster-content-count .poster-content-count-like,
.container .poster .poster-content .poster-content-count .poster-content-count-comment {
  display: flex;
  flex-direction: row;
  font-size: 26rpx;
  margin-left: 20rpx;
}
.container .poster .poster-content .poster-content-count .poster-content-count-like .poster-content-count-like-count,
.container .poster .poster-content .poster-content-count .poster-content-count-comment .poster-content-count-like-count,
.container .poster .poster-content .poster-content-count .poster-content-count-like .poster-content-count-comment-count,
.container .poster .poster-content .poster-content-count .poster-content-count-comment .poster-content-count-comment-count {
  color: #e41446;
}
.container .poster .poster-content .poster-content-count .poster-content-count-like .poster-content-count-like-text,
.container .poster .poster-content .poster-content-count .poster-content-count-comment .poster-content-count-like-text,
.container .poster .poster-content .poster-content-count .poster-content-count-like .poster-content-count-comment-text,
.container .poster .poster-content .poster-content-count .poster-content-count-comment .poster-content-count-comment-text {
  margin-left: 30rpx;
  color: #646262;
}
.container .poster .poster-content .poster-content-position {
  position: absolute;
  top: 200rpx;
  right: 50rpx;
}
.container .poster .poster-content .poster-content-position .poster-content-position-image {
  width: 180rpx;
  height: 240rpx;
}
.container .movie-info {
  padding-left: 20rpx;
  margin-top: 80rpx;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #dddddd;
}
.container .movie-info .movie-info-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #666;
}
.container .movie-info .movie-info-num {
  display: flex;
  flex-direction: row;
  margin-top: 60rpx;
  font-size: 28rpx;
}
.container .movie-info .movie-info-num .movie-info-num-star {
  margin-top: 15rpx;
  margin-left: 20rpx;
}
.container .movie-info .movie-info-num .movie-info-num-title {
  color: #a5a5a5;
}
.container .movie-info .movie-info-num .movie-info-num-average {
  margin-left: 40rpx;
  color: #2e426d;
}
.container .movie-info .movie-info-director,
.container .movie-info .movie-info-actors,
.container .movie-info .movie-info-type {
  margin-top: 30rpx;
  font-size: 28rpx;
}
.container .movie-info .movie-info-director .movie-info-director-title,
.container .movie-info .movie-info-actors .movie-info-director-title,
.container .movie-info .movie-info-type .movie-info-director-title,
.container .movie-info .movie-info-director .movie-info-actors-title,
.container .movie-info .movie-info-actors .movie-info-actors-title,
.container .movie-info .movie-info-type .movie-info-actors-title,
.container .movie-info .movie-info-director .movie-info-type-title,
.container .movie-info .movie-info-actors .movie-info-type-title,
.container .movie-info .movie-info-type .movie-info-type-title {
