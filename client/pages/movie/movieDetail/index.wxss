}
.container .movie-info .movie-info-director .movie-info-director-name,
.container .movie-info .movie-info-actors .movie-info-director-name,
.container .movie-info .movie-info-type .movie-info-director-name,
.container .movie-info .movie-info-director .movie-info-actors-name,
.container .movie-info .movie-info-actors .movie-info-actors-name,
.container .movie-info .movie-info-type .movie-info-actors-name,
.container .movie-info .movie-info-director .movie-info-type-name,
.container .movie-info .movie-info-actors .movie-info-type-name,
.container .movie-info .movie-info-type .movie-info-type-name {
  margin-left: 40rpx;
  color: #737373;
}
.container .movie-summary {
  margin-top: 40rpx;
  padding-left: 20rpx;
  display: flex;
  flex-direction: column;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #dddddd;
}
.container .movie-summary .movie-summary-title {
  color: #c2c2c2;
  font-size: 28rpx;
}
.container .movie-summary .movie-summary-text {
  margin-top: 20rpx;
  margin-right: 20rpx;
  color: #868686;
  font-size: 28rpx;
}
.container .actors-img {
  margin-top: 40rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  width: calc(100% - 40rpx);
}
.container .actors-img .actors-img-title {
  color: #c2c2c2;
  font-size: 28rpx;
}
.container .actors-img .actors-img-container {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  margin-top: 40rpx;
  padding-bottom: 40rpx;
  width: 100%;
}
.container .actors-img .actors-img-container .actors-img-item {
  margin: 20rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}
.container .actors-img .actors-img-container .actors-img-item .actors-img-item-image {
  width: 180rpx;
  height: 240rpx;
}
.container .actors-img .actors-img-container .actors-img-item .actors-img-item-name {
  color: #c2c2c2;
  font-size: 28rpx;
  text-align: center;
  white-space: normal;
}
